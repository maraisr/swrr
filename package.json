{
	"name": "swrr",
	"version": "0.0.6",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "index.js",
	"module": "index.mjs",
	"types": "index.d.ts",
	"files": [
		"index.*"
	],
	"scripts": {
		"build": "rollup -c",
		"format": "prettier --write --list-different \"{*,src/*,test/*,bench/*,.github/**/*}.+(ts|js|json|yml|md)\"",
		"test": "uvu test -r tsm \".spec.ts$\"",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"object-identity": "^0.0.4",
		"worktop": "^0.8.0-next.11"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.4.0",
		"@marais/tsconfig": "^0.0.2",
		"@types/node": "^17.0.21",
		"nanospy": "^0.5.0",
		"prettier": "^2.5.1",
		"rollup": "^2.68.0",
		"rollup-plugin-dts": "^4.1.0",
		"tsm": "^2.2.1",
		"typescript": "^4.5.5",
		"uvu": "^0.5.3"
	}
}