{
	"name": "swrr",
	"version": "0.0.6",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "index.js",
	"module": "index.mjs",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.js",
		"index.mjs"
	],
	"scripts": {
		"build": "bundt --minify",
		"format": "prettier --write src .github *.md *.json",
		"test": "uvu src \".test.ts$\" -r tsm",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"object-identity": "^0.0.4",
		"worktop": "^0.8.0-next.11"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "3.16.0",
		"@marais/prettier": "0.0.1",
		"@marais/tsconfig": "0.0.3",
		"@types/node": "18.7.18",
		"bundt": "2.0.0-next.0",
		"nanospy": "0.5.0",
		"prettier": "2.7.1",
		"tsm": "2.2.2",
		"typescript": "4.8.3",
		"uvu": "0.5.3"
	},
	"prettier": "@marais/prettier",
	"volta": {
		"node": "18.16.0"
	}
}
